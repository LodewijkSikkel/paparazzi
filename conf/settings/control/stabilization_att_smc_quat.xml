<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings>
  <dl_settings>

    <dl_settings NAME="smc">
      <dl_setting var="stabilization_attitude_deadband_tau" min="0" step="0.01" max="1" module="stabilization/stabilization_attitude_quat_smc" shortname="tau"/>

      <dl_setting var="stabilization_attitude_lambda_0.x" min="0" step=".1" max="100" module="stabilization/stabilization_attitude_quat_smc" shortname="lambda_0_x"/>
      <dl_setting var="stabilization_attitude_lambda_0.y" min="0" step=".1" max="100" module="stabilization/stabilization_attitude_quat_smc" shortname="lambda_0_y"/>
      <dl_setting var="stabilization_attitude_lambda_0.z" min="0" step=".1" max="100" module="stabilization/stabilization_attitude_quat_smc" shortname="lambda_0_z"/>

      <dl_setting var="stabilization_attitude_k.x" min="0" step="1" max="100" module="stabilization/stabilization_attitude_quat_smc" shortname="K_x"/>
      <dl_setting var="stabilization_attitude_k.y" min="0" step="1" max="100" module="stabilization/stabilization_attitude_quat_smc" shortname="K_y"/>
      <dl_setting var="stabilization_attitude_k.z" min="0" step="1" max="100" module="stabilization/stabilization_attitude_quat_smc" shortname="K_z"/>
    </dl_settings>

  </dl_settings>
</settings>
