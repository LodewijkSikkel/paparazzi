<!DOCTYPE module SYSTEM "module.dtd">

<module name="bluegiga" dir="com">
  <doc>
    <description>Communicate through BlueGiga SPI modules</description>
  </doc>
  <header>
    <file name="bluegiga.h"/>
  </header>
  <init fun="bluegiga_init()"/>
  <periodic fun="bluegiga_periodic()" freq="512" autorun="TRUE"/>
  <event fun="bluegiga_event()"/>
  <makefile>
    <file name="bluegiga.c"/>
  </makefile>
</module>

